namespace ba_timetracker.models.assets

import ba_timetracker.models.participants.*

asset Task identified by id {
  o String id
  o StateType state
  o String description
  o String comment
  o DateTime timeStart
  o Integer minutes
}

enum StateType {
  o BAG
  o FIX
  o TEST
}

asset TimeEntry identified by id {
  o String id
  o DateTime createAt
  o Long duration
  o String comment
  --> ParticipantEmployee employId 
  o String spentOn

}