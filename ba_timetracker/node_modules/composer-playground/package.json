      "e2e/data/files/importReadMe.md"
    ],
    "output": "/Users/user/Desktop/playground-output.txt",
    "file_type_method": "EXCLUDE",
    "file_types": [
      ".yml",
      ".yaml",
      ".txt",
      ".bna",
      ".tgz",
      ".gz",
      ".pem",
      ".key",
      ".crt",
      ".card",
      ".tx",
      ".block"
    ],
    "insert_license": false,
    "default_format": {
      "eachLine": {
        "prepend": "# "
      }
    },
    "license_formats": {
      "js|ts|scss|cto|acl|qry": {
        "prepend": "/*",
        "append": " */",
        "eachLine": {
          "prepend": " * "
        }
      },
      "gitkeep|cfignore|nvmrc|npmrc|editorconfig|ppc64le": {
        "eachLine": {
          "prepend": "# "
        }
      },
      "md": {
        "file": "../../markdown-license.txt"
      }
    }
